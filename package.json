{
  "name": "@better-slop/ocx",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*",
    "internals/*"
  ],
  "catalog": {
    "@opencode-ai/plugin": "latest",
    "@types/bun": "latest",
    "typescript": "5.9.3",
    "handlebars": "4.7.8",
    "effect": "latest"
  },
  "scripts": {
    "compile:scripts": "bun build scripts/* --compile --out dist/scripts",
    "deploy:docs": "bun run --filter docs deploy",
    "destroy:docs": "bun run --filter docs destroy",
    "deploy": "bun run deploy:docs",
    "destroy": "bun run destroy:docs"
  },
  "devDependencies": {
    "@types/bun": "catalog:",
    "typescript": "catalog:",
    "@better-slop-internals/tsconfig": "workspace:*"
  },
  "patchedDependencies": {
    "alchemy@0.79.0": "patches/alchemy@0.79.0.patch"
  }
}
